<p id="notice"><%= notice %></p>

<h1>Listing Polls</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>

      <th>End</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polls.each do |poll| %>
      <tr>
        <td><%= poll.title %></td>
        <td><%= poll.end %></td>
        <td><%= link_to 'Edit poll', edit_poll_path(poll) %></td>
        <td>Vote</td>
      </tr>
      <tr></tr>

    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create a New Poll', new_poll_path %>
